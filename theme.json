{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#968ffb",
					"name": "Primary 100",
					"slug": "primary-100"
				},
				{
					"color": "#8a82fb",
					"name": "Primary 200",
					"slug": "primary-200"
				},
				{
					"color": "#7c75e2",
					"name": "Primary 300",
					"slug": "primary-300"
				},
				{
					"color": "#6e68c9",
					"name": "Primary 400",
					"slug": "primary-400"
				},
				{
					"color": "#615bb0",
					"name": "Primary 500",
					"slug": "primary-500"
				},
				{
					"color": "#f1f5f9",
					"name": "Slate 100",
					"slug": "slate-100"
				},
				{
					"color": "#e2e8f0",
					"name": "Slate 200",
					"slug": "slate-200"
				},
				{
					"color": "#cbd5e1",
					"name": "Slate 300",
					"slug": "slate-300"
				},
				{
					"color": "#94a3b8",
					"name": "Slate 400",
					"slug": "slate-400"
				},
				{
					"color": "#64748b",
					"name": "Slate 500",
					"slug": "slate-500"
				},
				{
					"color": "#475569",
					"name": "Slate 600",
					"slug": "slate-600"
				},
				{
					"color": "#334155",
					"name": "Slate 700",
					"slug": "slate-700"
				},
				{
					"color": "#1e293b",
					"name": "Slate 800",
					"slug": "slate-800"
				},
				{
					"color": "#0f172a",
					"name": "Slate 900",
					"slug": "slate-900"
				},
				{
					"color": "#000000",
					"name": "Color 1",
					"slug": "custom-color-1"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1440px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"spacingScale": {
				"increment": 2,
				"mediumStep": 2.5,
				"operator": "*",
				"steps": 50,
				"unit": "px"
			},
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFamily": "\"scandia-web\", sans-serif",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Recoleta Alt\"",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./fonts/Recoleta-Regular.ttf"
							]
						}
					],
					"fontFamily": "\"Recoleta Alt\"",
					"name": "Secondary",
					"slug": "secondary"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "12px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "14px",
					"slug": "medium"
				},
				{
					"name": "Base",
					"size": "18px",
					"slug": "large"
				},
				{
					"name": "Large",
					"size": "22px",
					"slug": "x-large"
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var:preset|color|slate-800",
			"text": "var:preset|color|slate-200"
		},
		"elements": {
			"button": {
				"color": {
					"background": "#cbd5e1",
					"text": "var:preset|color|slate-900"
				},
				"typography": {
					"fontFamily": "\"scandia-web\", sans-serif",
					"fontSize": "1.125rem"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "Recoleta Alt"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "Recoleta Alt",
					"fontSize": "1.375rem"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "'Recoleta Alt'"
				}
			},
			"link": {
				"typography": {
					"fontFamily": "'scandia-web', sans-serif",
					"fontSize": "1.125rem"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|primary",
			"fontSize": "var:preset|font-size|large",
			"fontStyle": "normal",
			"fontWeight": "700"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}